{
  "name": "@design-view/helper",
  "type": "module",
  "version": "1.7.5",
  "description": "My arsenal. (工具集军火库)",
  "private": false,
  "scripts": {
    "coverage": "vitest run --coverage",
    "build": "unbuild --minify",
    "dev": "unbuild --stub",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:eslint": "eslint . --fix",
    "lint:format": "prettier --write --loglevel warn \"**/*.{js,cjs,ts}\"",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.cjs",
    "lint:all": "pnpm run lint:eslint && pnpm run lint:format",
    "doc": "typedoc --options typedoc.json",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp --commit \"release: release v%s\" --push --tag && npm publish",
    "npm:up": "pnpx npm-check-updates",
    "test:watch": "vitest --watch",
    "test": "vitest run",
    "prepare": "husky install"
  },
  "homepage": "https://github.com/kailong321200875/helper",
  "bugs": {
    "url": "https://github.com/kailong321200875/helper/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kailong321200875/helper.git"
  },
  "keywords": [
    "tool",
    "utils",
    "helper",
    "typescript"
  ],
  "author": "Archer <502431556@qq.com>",
  "license": "ISC",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./string": {
      "types": "./dist/string.d.ts",
      "import": "./dist/string.mjs",
      "require": "./dist/string.cjs"
    },
    "./is": {
      "types": "./dist/is.d.ts",
      "import": "./dist/is.mjs",
      "require": "./dist/is.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "devDependencies": {
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "@vitest/coverage-v8": "^1.1.3",
    "bumpp": "^9.2.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^8.0.3",
    "jsdom": "^23.2.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.1",
    "typedoc": "^0.25.7",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "unplugin-auto-import": "^0.17.3",
    "vitest": "^1.1.3"
  },
  "packageManager": "pnpm@8.1.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.1.0"
  }
}
