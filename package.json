{
  "name": "tool-helper",
  "version": "1.4.3",
  "description": "Collect commonly used toolsets. (收集常用的工具集)",
  "packageManager": "pnpm@7.0.1",
  "private": false,
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup -c",
    "dev": "cross-env NODE_ENV=development rollup -c --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:eslint": "eslint . --fix",
    "lint:format": "prettier --write --loglevel warn \"**/*.{js,ts}\"",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "lint:all": "pnpm run lint:eslint && pnpm run lint:format",
    "doc": "typedoc --options typedoc.json",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp --commit \"release: release v%s\" --push --tag && npm publish",
    "npm:up": "npx npm-check-updates",
    "test": "vitest",
    "prepare": "husky install"
  },
  "homepage": "https://github.com/kailong321200875/tool-helper",
  "bugs": {
    "url": "https://github.com/kailong321200875/tool-helper/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kailong321200875/tool-helper.git"
  },
  "keywords": [
    "tool",
    "utils",
    "helpers",
    "typescript",
    "tool-helper"
  ],
  "author": "Archer <502431556@qq.com>",
  "license": "ISC",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./index.d.ts"
    }
  },
  "files": [
    "dist",
    "*.d.ts"
  ],
  "sideEffects": false,
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@types/node": "^18.6.1",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "babel-preset-env": "^1.7.0",
    "bumpp": "^8.2.1",
    "cross-env": "^7.0.3",
    "esbuild": "^0.14.50",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jsdom": "^20.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "rollup": "^2.77.2",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-esbuild": "^4.9.1",
    "rollup-plugin-typescript2": "^0.32.1",
    "tslib": "^2.4.0",
    "typedoc": "^0.23.9",
    "typescript": "^4.7.4",
    "vitest": "^0.19.1"
  }
}
